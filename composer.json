{
  "name": "signalwire/signalwire",
  "description": "Client library for connecting to SignalWire.",
  "version": "1.1.1",
  "type": "library",
  "keywords": [
    "signalwire",
    "laml"
  ],
  "homepage": "https://github.com/signalwire/signalwire-php",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "SignalWire\\": "src/"
    },
    "files": [
      "src/functions.php"
    ]
  },
  "authors": [
    {
      "name": "SignalWire Team",
      "email": "open.source@signalwire.com"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/lpradovera/php-vcr"
    }
  ],
  "require": {
    "twilio/sdk": "^5.21.3",
    "ratchet/pawl": "^0.3.4",
    "monolog/monolog": "^1.24"
  },
  "require-dev": {
    "phpunit/phpunit": "^7",
    "php-vcr/php-vcr": "dev-master",
    "mockery/mockery": "^1.2"
  }
}
